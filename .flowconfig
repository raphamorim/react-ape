[ignore]

[include]

[libs]

[lints]
all=warn
sketchy-null=off
untyped-import=off

[options]
module.use_strict=true
suppress_type=$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
include_warnings=true
emoji=true

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
